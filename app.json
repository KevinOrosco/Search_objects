{
  "expo": {
    "name": "Busquemos objetos",
    "slug": "object-finder-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-pingu.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pinguino.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-pingu.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.koroscocespedes.objectfindergame"
    },
    "web": {
      "favicon": "./assets/icon-pingu.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "La aplicación necesita acceso a tu cámara para buscar objetos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6f427b0d-fc69-4825-bc98-514a3e2e29ae"
      }
    }
  }
}
